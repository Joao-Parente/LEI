<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<!-- Form -->
	<record id="view_form_plano_estudos_uc" model="ir.ui.view">
		<field name="name">Plano de Estudos UC Form</field>
		<field name="model">transum.plano_estudos_uc</field>
		<field name="arch" type="xml">
			<form>
				<header></header>
				<sheet>
					<group name="group_top">
						<field name="uc" />
						<field name="creditacao" />
						<field name="nota" />
					</group>
				</sheet>
			</form>
		</field>
	</record>
	<!-- Tree -->
	<record id="view_tree_uc" model="ir.ui.view">
		<field name="name">Lista de Plano de Estudos UC</field>
		<field name="model">transum.plano_estudos_uc</field>
		<field name="arch" type="xml">
			<tree>
				<field name="uc" />
				<field name="creditacao" />
				<field name="nota" />
			</tree>
		</field>
	</record>
</odoo>