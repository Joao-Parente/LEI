<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<!-- Form -->
	<record id="view_form_proposta_novo_plano" model="ir.ui.view">
		<field name="name">Proposta Novo Plano Form</field>
		<field name="model">transum.proposta_novo_plano</field>
		<field name="arch" type="xml">
			<form>
				<header>
					<!-- <button name="aprovar" type="object" string="Aprovar" class="oe_highlight"/>
					<button name="rejeitar" type="object" string="Rejeitar" class="oe_highlight"/> -->
				</header>
				<sheet>
					<group name="group_top">
						<group>
							<field name="plano_antigo" />
							<field name="plano_transicao" />
						</group>
						<group>
							<field name="planos_novos" />
						</group>
					</group>
				</sheet>
			</form>
		</field>
	</record>
	<!-- Tree -->
	<record id="view_tree_proposta_novo_plano" model="ir.ui.view">
		<field name="name">Lista de Proposta Novo Plano</field>
		<field name="model">transum.proposta_novo_plano</field>
		<field name="arch" type="xml">
			<tree>
				<field name="plano_transicao"/>
			</tree>
		</field>
	</record>
</odoo>